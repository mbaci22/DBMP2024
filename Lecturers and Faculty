CREATE TABLE Lecturers (
  LecturerName VARCHAR(100) ,
  LecturerSurname VARCHAR(100) ,
  LecturerID INT PRIMARY KEY , 
  LecturerNationalID VARCHAR(50) PRIMARY KEY , 
  LecturerBirthplace VARCHAR(100), 
  LecturerBirthday DATE , 
  LecturerSex VARCHAR(50) , 
  LecturerEmail VARCHAR(200), 
  LecturerCitizenship VARCHAR(100), 
  FacultyID INT FOREIGN KEY
  );
  
  CREATE TABLE Faculty (
    FacultyID INT PRIMARY KEY,
    FacultyName VARCHAR(50) , 
    LecturersID INT FOREIGN KEY
    );
